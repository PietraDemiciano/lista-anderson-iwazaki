algoritmo "numero_por_extenso"
var
  num: inteiro
inicio
  escreva("Digite um número entre 1 e 100: ")
  leia(num)
  escolha num
    caso 1: escreval("Um")
    caso 2: escreval("Dois")
    ...
    caso 20: escreval("Vinte")
    outrocaso: escreval("Número fora do exemplo")
  fimescolha
fimalgoritmo
